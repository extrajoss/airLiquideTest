        borderleft="border-left:solid 0.2em #333333;"
        leftBorder = false;
      }     
        var link = proteinListGoogleSpreadsheet[element][ligand][energy];%>
                      <td style="<%=borderleft%>" onclick="selectGoogleSpreadsheet(this,'<%=link%>');"><img src="/assets/images/sheets-logo.png" height="50" width="45" title="⇲ <%=element%> <%=ligand%> <%=energy%>"
                          alt="⇲ <%=element%> <%=ligand%> <%=energy%>"></img>
                      </td>
                      <% });%>
              </tr>
              <%}); %>
