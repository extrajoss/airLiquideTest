<body>
    <div class="container">
          <!-- show any messages that come back with authentication -->
      <form action="/forgotPassword" method="post" class="form-signin">
      <div class="panel">
        <h3 class="form-signin-heading">Reset Password</h3>
        <div class="form-group">
        <label for="email" class="sr-only">Email</label>
        <input type="email" id="email" name="email" class="form-control" placeholder="email" value="<%= email %>" required autofocus/>
        <% if (message.length > 0) { %>
          <div class="alert alert-danger"><%= message %></div>
        <% } %>
      </div>
         <button class="btn btn-lg btn-block" type="submit">Reset Password</button>
         <p>Don't have an account?
            <a href="/register">Register</a>
        </p>
        <p>Or go
            <a href="/">home</a>.</p>
         </div>
      </form>
    </div>
</body>

</html>